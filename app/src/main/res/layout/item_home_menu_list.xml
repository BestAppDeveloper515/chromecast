<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/homeItemMenu"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_100ROR"
    android:clickable="true"
    android:padding="8dp">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/container_item_menu_list"
        >


    <app.rayscast.air.ui.SquareImageView
        android:id="@+id/meenuImage"
        android:layout_width="@dimen/_56ROR"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:layout_gravity="center"
        android:src="@drawable/audio_hdpi" />

    <TextView
        android:id="@+id/menuTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/meenuImage"
        android:gravity="center"
        android:layout_gravity="center_horizontal"
        android:text="Audio"
        android:textColor="#000000"

        />
    </LinearLayout>


</RelativeLayout>